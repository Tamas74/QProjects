<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	
	<!--Style -->

	<LinearGradientBrush x:Key="InfoBackgroundHeader" EndPoint="0.5,1" StartPoint="0.5,0">
		 <GradientStop Color="#FF8EB3E7"/>
        <GradientStop Color="#FF799BCA" Offset="0.956"/>
	</LinearGradientBrush>

    <SolidColorBrush x:Key="InfoBorderBrush" Color="#FF1F497D"/>
	
	<SolidColorBrush x:Key="InfoForegroundBrush" Color="#FF1F497D"/>
	
	<SolidColorBrush x:Key="InfoForegroundBrushHeaderText" Color="White"/>
	

    <LinearGradientBrush x:Key="InfoBackgroundBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFF8FAFE" />
        <GradientStop Color="#FFCFE0F8"
                      Offset="1" />
    </LinearGradientBrush>
	
	<SolidColorBrush x:Key="InfoBaseBackgroundBrush" Color="#FFCFE0F8"/>
    
    <SolidColorBrush x:Key="InfoForegroundBrushText" Color="#FF1F497D"/>
	
    
</ResourceDictionary>